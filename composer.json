{
  "name": "camilord/laravel-api-controller",
  "description": "A laravel api base controller with basic CRUD mapped to the model. Fork from https://github.com/phpsa/laravel-api-controller ",
  "license": "MIT",
  "keywords": [
    "laravel",
    "api",
    "controller"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Craig G Smith",
      "email": "vxdhost@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "laravel/framework": ">=8.0|>=9.0|>=10.0"
  },
  "require-dev": {
    "orchestra/testbench": ">=8.0|>=9.0",
    "phpunit/phpunit": ">=9.0"
  },
  "autoload": {
    "psr-4": {
      "Phpsa\\LaravelApiController\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phpsa\\LaravelApiController\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Phpsa\\LaravelApiController\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
